{
    "name": "96-well Plate",
    "description": "A standard 96-well plate that conforms to ISOxxx",
    "backgroundImage": "96-well-plate.svg",
    
    "wells": [
	{ "id": "A1", "x": 1, "y": 1, "shape": "circle", "diameter": 1 },
    ],

    "selectors": [
	{ "description": "Select column A1 to H1", "x": 2, "y": 2, "selects": "[A-H]1" },
	{ "description": "Select column A2 to H2", "x": 3, "y": 3, "selects": "[A-H]2" },
	{ "description": "Select All", "x": 4, "y": 4, "selects": ".*" },
    ],

    "tableArrangements": [
	{
	    "description": "Arrange by row",
	    "arrangement": ["A1", "A2"...]
	},
	{
	    "description": "Arrange by column",
	    "arrangement": ["A1", "B1"...]
	},
    ],
}
